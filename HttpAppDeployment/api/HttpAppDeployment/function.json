{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "appDeployments",
      "collectionName": "documents",
      "createIfNotExists": false,
      "connectionStringSetting": "bysoutcosmos_DOCUMENTDB"
    }
  ],
  "scriptFile": "../dist/HttpAppDeployment/index.js"
}