{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocument",
      "databaseName": "byrsouthdb",
      "collectionName": "appDocuments",
      "connectionStringSetting": "byrsouthdb_DOCUMENTDB",
      "id": "id"
     
    }
  ],
  "scriptFile": "../dist/HttpAppDeploymentOut/index.js"
}