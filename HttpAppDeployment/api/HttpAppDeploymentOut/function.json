{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocument",
      "databaseName": "DeployDataDB",
      "collectionName": "deployments",
      "connectionStringSetting": "byrsouthdb_DOCUMENTDB",
      "partitionKey": "id"
    }
  ],
  "scriptFile": "../dist/HttpAppDeploymentOut/index.js"
}